# Example environment variables for Relay project
# Copy this file to .env and fill in your actual secrets

# Weatherstack API key for weather data
WEATHERSTACK_API_KEY=your_weatherstack_api_key_here


# Notion API key for Notion integration
NOTION_API_KEY=your_notion_api_key_here
# Notion target page ID
NOTION_PAGE_ID=your_notion_page_id_here

# Discord bot token and channel ID
BOT_TOKEN=your_discord_bot_token_here
DISCORD_REPORT_CHANNEL_ID=your_discord_channel_id_here

# Cerebras API key for LLM integration
CEREBRAS_API_KEY=your_cerebras_api_key_here
